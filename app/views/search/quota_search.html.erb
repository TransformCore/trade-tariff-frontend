<%= render partial: 'search/quotas/form', locals: { search_params: @result[:params] } %>
<% if @result[:quotas]&.any? %>
  <h1 class="heading-medium">Quota search results</h1>
  <article class="search-results">
      <table class="tariff-zebra index">
        <thead>
          <tr>
            <td>Order number</td>
            <td>Origins</td>
            <td>Start date</td>
            <td>End date</td>
            <td>Balance</td>
            <td></td>
          </tr>
        </thead>
        <tbody>
          <%= render partial: 'search/quotas/definition', collection: @result[:quotas] %>
        </tbody>
      </table>
  </article>
<% end %>
